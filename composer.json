{
    "name": "chelsymooy/laravel-approval",
    "description": "chelsymooy approval designed for approvals using inboxes.",
    "type": "library",
    "keywords": [
        "laravel",
        "feature",
        "database",
        "approval"
    ],
    "license": "MIT",
    "homepage": "https://thunderlab.id",
    "support": {
        "email": "chelsy@thunderlab.id",
        "issues": "https://github.com/chelsymooy/laravel-approval/issues",
        "source": "https://github.com/chelsymooy/laravel-approval",
        "docs": "https://github.com/chelsymooy/laravel-approval/blob/master/README.md"
    },
    "authors": [
        {
            "name": "chelsymooy",
            "homepage": "https://thunderlab.id",
            "email": "chelsy@thunderlab.id"
        }
    ],
    "require": {
        "php": "^7.4.0",
        "illuminate/console": "^7.0.0",
        "illuminate/database": "^7.0.0",
        "illuminate/support": "^7.0.0"
    },
    "require-dev": {
        "illuminate/container": "^7.0.0",
        "phpunit/phpunit": "^9.0.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Chelsymooy\\Approval\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chelsymooy\\Approval\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chelsymooy\\Approval\\Providers\\ApprovalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
